{
  "name": "polykey",
  "version": "0.0.1",
  "description": "Polykey Core Library",
  "keywords": [
    "secrets"
  ],
  "author": "Roger Qiu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-polykey.git"
  },
  "main": "dist/index.node.cjs.js",
  "module": "dist/index.node.es.js",
  "browser": "dist/index.browser.umd.js",
  "scripts": {
    "test": "ava -v",
    "doc": "documentation build ./lib/** -f html -o ./doc",
    "rollup": "rollup --config",
    "dos2unix": "dos2unix dist/*",
    "build": "npm test && npm run doc && npm run rollup && npm run dos2unix"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "documentation": "^8.0.0",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "isomorphic-git": "^0.18.2",
    "secure-random-bytes": "^1.0.1",
    "virtualfs": "^2.1.0"
  }
}
